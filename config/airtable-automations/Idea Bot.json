      "parameters": {
        "jsCode": "// 1. Get content\nconst rawContent = $input.first().json.message?.content || $input.first().json.content || $input.first().json.output;\n\nif (!rawContent) return { json: { error: \"No content\" } };\n\n// 2. Clean Markdown\nlet cleanContent = rawContent.replace(/```json/g, '').replace(/```/g, '').trim();\n\n// 3. Parse and Return Options\ntry {\n  let parsedData = JSON.parse(cleanContent);\n  \n  // SELF-HEALING LOGIC:\n  // If the AI output a single object with no \"concepts\" key, wrap it.\n  if (!parsedData.concepts && parsedData.viral_title) {\n     parsedData = { concepts: [parsedData] };\n  } \n  // If the AI output an array directly (without the \"concepts\" key), fix it.\n  else if (Array.isArray(parsedData)) {\n     parsedData = { concepts: parsedData };\n  }\n\n  // Now process strictly\n  if (parsedData.concepts && Array.isArray(parsedData.concepts)) {\n    return {\n      json: {\n        option_1: parsedData.concepts[0] || null,\n        option_2: parsedData.concepts[1] || null, // Will be null if AI only sent 1\n        option_3: parsedData.concepts[2] || null,\n        full_data: parsedData\n      }\n    };\n  } else {\n    return { json: { error: \"Structure Invalid\", raw: parsedData } };\n  }\n\n} catch (error) {\n  return { json: { error: \"Parse Failed\", raw: rawContent } };\n}"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
